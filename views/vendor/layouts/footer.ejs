<script>
    const notyf = new Notyf();
    // Define the alertNotification function
    function alertNotification(status, msg) {
        // Check the status and call the corresponding Notyf method
        if (status === "success") {
            notyf.success({
                message: msg,
                duration: 5000
            });
        } else if (status === "error") {
            notyf.error({
                message: msg,
                duration: 5000
            });
            notifyRemove()
        } else {
            // For custom types or warnings
            notyf.open({
                type: status, // Add custom types as needed
                message: msg,
                duration: 5000
            });
        }
    }

    function notifyRemove() {
        const notyfAlertRemove = document.querySelectorAll(".notyf__toast")
        for (let i = 0; i < notyfAlertRemove.length; i++) {
            notyfAlertRemove[i].addEventListener("click", function () {
                notyfAlertRemove[i].style.display = "none"
                console.log([i])
            })
        }

    }


</script>
